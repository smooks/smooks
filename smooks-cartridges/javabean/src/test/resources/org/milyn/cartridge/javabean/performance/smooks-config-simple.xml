<?xml version="1.0"?>
<smooks-resource-list xmlns="http://www.milyn.org/xsd/smooks-1.0.xsd">
	
	<resource-config selector="global-parameters">
        <param name="stream.filter.type">SAX</param>
    </resource-config>

    <resource-config selector="person">
        <resource>org.milyn.cartridge.javabean.BeanPopulator</resource>
        <param name="beanId">person</param>
        <param name="beanClass">org.milyn.cartridge.javabean.performance.model.Person</param>
        <param name="bindings">
        	<binding property="surname" type="String" selector="person" />
		</param>
    </resource-config>
     
    
</smooks-resource-list>