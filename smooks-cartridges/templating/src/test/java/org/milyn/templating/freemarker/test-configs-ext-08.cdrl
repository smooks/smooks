<?xml version="1.0"?>
<smooks-resource-list xmlns="http://www.milyn.org/xsd/smooks-1.1.xsd"
                      xmlns:ftl="http://www.milyn.org/xsd/smooks/freemarker-1.1.xsd"
                      xmlns:jb="http://www.milyn.org/xsd/smooks/javabean-1.1.xsd">

    <resource-config selector="$document">
        <resource>org.milyn.delivery.DomModelCreator</resource>
    </resource-config>

    <ftl:freemarker applyOnElement="$document">
        <ftl:template><!--<#ftl ns_prefixes={"acme":"http://acme.com"}><#foreach c in a["acme:b/acme:c"]>'${c}'</#foreach> javaVal=${x.val}--></ftl:template>
    </ftl:freemarker>

    <jb:bindings beanId="x" class="java.util.HashMap" createOnElement="b">
        <jb:value property="val" data="b/@javabind" />
    </jb:bindings>

</smooks-resource-list>
