<?xml version="1.0"?>
<smooks-resource-list xmlns="http://www.milyn.org/xsd/smooks-1.1.xsd" xmlns:jb="http://www.milyn.org/xsd/smooks/javabean-1.4.xsd">

    <jb:bean beanId="myBeanCData" class="org.smooks.templating.MyBean" createOnElement="c">
        <jb:value property="x" data="c/@x" />
    </jb:bean>

    <jb:bean beanId="myBeanEData" class="org.smooks.templating.MyBean" createOnElement="e">
        <jb:value property="x" data="e/@x" />
    </jb:bean>

    <resource-config selector="$document">
        <resource>org.smooks.templating.MockOutStreamResource</resource>
        <param name="resourceName">mockOutStream</param>
    </resource-config>

    <resource-config selector="d">
        <resource>/org/smooks/templating/stringtemplate/test-template-2.st</resource>
        <param name="outputStreamResource">mockOutStream</param>
        <param name="applyTemplateBefore">true</param>
    </resource-config>

    <resource-config selector="e">
        <resource>/org/smooks/templating/stringtemplate/test-template-3.st</resource>
        <param name="outputStreamResource">mockOutStream</param>
    </resource-config>

</smooks-resource-list>
